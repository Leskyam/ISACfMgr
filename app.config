    <LAMSoft.ISACFMgr.Properties.Settings>
      <setting name="downloaderUserAgent" serializeAs="String">
        <value>ISA Categorization &amp; Content Filter Manager/1.0</value>
      </setting>
      <setting name="downloadUri" serializeAs="String">
        <value>http://localhost/test-isacfmgr/shallalist.tar.gz</value>
