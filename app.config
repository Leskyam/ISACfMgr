    </sectionGroup>
  </configSections>
  <connectionStrings>
    <add name="LAMSoft.ISACFMgr.Properties.Settings.ISA_Server_LogsConnectionString" connectionString="Data Source=(local);Initial Catalog=ISA_Server_Logs;Persist Security Info=True;User ID=sa;Password=sapass;Application Name=&quot;ISACFMgr Service&quot;" providerName="System.Data.SqlClient"/>
